{
  "version": 2,
  "functions": {
    "node.js": {
      "memory": 3008,
      "maxDuration": 30
    },
    "/*.js": {
      "memory": 3008,
      "maxDuration": 30
    }
  },
  "builds": [
    {
      "src": "app.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ]
}
